function y = initialiseState(x)
 y=cell(800,1);
for i=1:8
    if i==1
    temp=zeros(1,3);
    end
    if i==2
    temp=[0,0,1];
    end
    if i==3
    temp=[0,1,0];
    end
    if i==4
    temp=zeros(1,3);
    end
        if i==i
    temp=zeros(1,3);
        end
        if i==i
    temp=zeros(1,3);
    end
        for j=1:M
            y((i-1)*100+j)=[x{j},temp];
        end
end